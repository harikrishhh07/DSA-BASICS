#include <iostream>
using namespace std;
int main()
{
    int size;
    cout << "Enter the size of the array: ";
    cin >> size;
    int arr[size];
    for (int i = 0; i < size; i++) {
        cout << "Enter element " << i + 1 << ": ";
        cin >> arr[i];
    }
    for (int i = 0; i < size; i++) {
        for (int z = 0; z < size - i - 1; z++) {
            if (arr[z] > arr[z + 1]) {
                swap(arr[z], arr[z + 1]);
            }
        }
    }
    cout << "The Bubble Sorted Array is: ";
    for (int i = 0; i < size; i++) {
        cout << arr[i] << " ";
    }
    return 0;
}
